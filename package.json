{
  "name": "use-query-state-react-query",
  "version": "1.0.0",
  "description": "This library so as to manage global state for React Query.",
  "type": "module",
  "author": "Karukichi Soejima",
  "keywords": [
    "react-query",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yota-K/use-query-state-react-query"
  },
  "bugs": {
    "url": "https://github.com/Yota-K/use-query-state-react-query/issues"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "format": "prettier --write **/*.{ts,tsx}",
    "test": "vitest"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=4.0.5",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.3.9",
    "@tanstack/react-query-devtools": "^4.3.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "happy-dom": "^6.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  },
  "files": [
    "/dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  }
}
